SET AX 9
IO_FS_CREATE FS file1.fs
IO_FS_TRUNCATE FS file1.fs AX
IO_FS_CREATE FS file2.fs
IO_FS_TRUNCATE FS file2.fs AX
IO_FS_CREATE FS file3.fs
IO_FS_TRUNCATE FS file3.fs AX
SET CX 0
SET BX 4
RESIZE 4
SET AX 0
SET DX 5
IO_FS_WRITE FS file1.fs AX BX CX
IO_FS_WRITE FS file1.fs AX BX DX
IO_FS_WRITE FS file2.fs AX BX CX
IO_FS_WRITE FS file2.fs AX BX DX
IO_FS_WRITE FS file3.fs AX BX CX
IO_FS_WRITE FS file3.fs AX BX DX
SET AX 17 
IO_FS_TRUNCATE FS file2.fs AX
EXIT